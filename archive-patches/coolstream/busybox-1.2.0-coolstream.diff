+	if (cmd == 8) {
+		if (klogctl(cmd, NULL, level) < 0)
+			goto die_the_death;
+		goto all_done;
+	}
+
+	buf = xmalloc(bufsize);
+	if ((n = klogctl(cmd, buf, bufsize)) < 0)
+		goto die_the_death;
+
+	lastc = '\n';
+	for (i = 0; i < n; i++) {
+		if (lastc == '\n' && buf[i] == '<') {
+			i++;
+			while (buf[i] >= '0' && buf[i] <= '9')
