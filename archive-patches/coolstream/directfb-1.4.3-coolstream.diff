+     VoodooInstanceID instance;
+
+     DIRECT_ALLOCATE_INTERFACE_DATA(thiz, IDirectFBFoo_Dispatcher)
+
+     D_ASSERT( real != NULL );
+     D_ASSERT( manager != NULL );
+     D_ASSERT( super != VOODOO_INSTANCE_NONE );
+     D_ASSERT( ret_instance != NULL );
