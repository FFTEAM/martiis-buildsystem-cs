+#ifndef __UC_PROBE_H__
+#define __UC_PROBE_H__
+
+struct uc_vga_device 
+{
+    u32 busid;      /* bus<8>:dev<5>:sub<3> */
+    u32 devid;      /* mfr<16>:device<16> */
+    u32 irq;        /* IRQ number */
+
+    u32 fbmem;      /* Framebuffer base address */
+    u32 fbsize;     /* Framebuffer size in bytes */
